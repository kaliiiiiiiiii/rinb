{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Config",
  "type": "object",
  "properties": {
    "arch": {
      "$ref": "#/$defs/Arch",
      "default": "amd64"
    },
    "editon": {
      "type": "string",
      "default": "Professional"
    },
    "lang": {
      "description": "Lang code, e.g. en-US",
      "type": "string",
      "default": "en-US"
    },
    "version": {
      "$ref": "#/$defs/WinVer",
      "default": "11"
    }
  },
  "$defs": {
    "Arch": {
      "type": "string",
      "enum": [
        "amd64",
        "arm64"
      ]
    },
    "WinVer": {
      "type": "string",
      "enum": [
        "10",
        "11"
      ]
    }
  }
}